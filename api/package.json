{
	"name": "api",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "npm run prod",
		"watch:dev": "nodemon",
		"test": "jest",
		"build": "npm-run-all clean transpile",
		"server": "node ./dist-server/bin/www",
		"dev": "NODE_ENV=development npm-run-all build server",
		"prod": "NODE_ENV=production npm-run-all build server",
		"transpile": "babel ./server --out-dir dist-server",
		"clean": "rimraf dist-server",
		"add-demos": "npx sequelize-cli db:seed:all",
		"remove-demos": "npx sequelize-cli db:seed:undo:all"
	},
	"dependencies": {
		"@babel/cli": "^7.17.6",
		"@babel/core": "^7.17.9",
		"@babel/preset-env": "^7.16.11",
		"@babel/runtime": "^7.17.9",
		"cookie-parser": "~1.4.4",
		"debug": "~2.6.9",
		"dotenv": "^16.0.0",
		"express": "~4.16.1",
		"morgan": "~1.9.1",
		"nodemon": "^2.0.15",
		"npm-run-all": "^4.1.5",
		"pg": "^8.7.3",
		"pg-hstore": "^2.3.4",
		"rimraf": "^3.0.2",
		"sequelize": "^6.18.0",
		"winston": "^3.7.2"
	},
	"nodemonConfig": {
		"exec": "npm run dev",
		"watch": [
			"server/*",
			"public/*"
		],
		"ignore": [
			"**/__tests__/**",
			"*.test.js",
			"*.spec.js"
		]
	},
	"jest": {
		"testEnvironment": "node"
	},
	"devDependencies": {
		"@babel/plugin-transform-runtime": "^7.17.0",
		"jest": "^27.5.1",
		"sequelize-cli": "^6.4.1"
	}
}
